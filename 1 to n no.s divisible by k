#include<stdio.h>
void main()
{
    int i,n,k,evensum=0,oddsum=0;
    printf("enter n value:");
    scanf("%d",&n);
     printf("enter k value:");
     scanf("%d",&k);
     for(i=1;i<=n;i++)
     {
     if(i%k==0)
     {
         printf("%d\n",i);
     }
     }
     if(i%2==0)
      {
      evensum=evensum+i;
      }
      if(i%2!=0)
      {
      oddsum=oddsum+i;
      }
          printf("sum of odd numbers:%d\n",oddsum);
          printf("sum of even numbers:%d\n",evensum);
}
